sprites/players/raio_rage_torso
{
	nomipmaps
	nopicmip
	{
		texFilter nearest
		map textures/sprites/players/raio_torso.dds
		blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
		rgbGen wave sin 1.0 1.0 1.0 0.1 0.15 0.01
		stage diffuseMap
	}
	{
		texFilter nearest
		map textures/sprites/players/raio_torso_n.dds
		stage normalMap
	}
}

