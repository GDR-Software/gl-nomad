sprites/players/raio_base
{
    nopicmip
    nomipmaps
    {
        texFilter nearest
        map textures/sprites/raio_base.tga
        blendFunc GL_SRC_ALPHA GL_ONE_MINUS_SRC_ALPHA
        rgbGen vertex
    }
}

